{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        },
        {
          "key": "X-DNS-Prefetch-Control",
          "value": "on"
        }
      ]
    }
  ],
  "functions": {
    "app/**": {
      "maxDuration": 10
    }
  }
}
